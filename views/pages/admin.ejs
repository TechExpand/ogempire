        <div class="col-lg-12">
          <div class="invest-table-area style--two wow fadeInUp" data-wow-duration="0.5s" data-wow-delay="0.5s">
            <table>
              <thead>
                <tr>
                    <th>Name</th>
                        <th>Email</th>
                        <th>Total Profit</th>
                        <th>Total Deposit</th>
                        <th>Total Withdraw</th>
                </tr>
              </thead>
              <tbody>

                <% for(var i=0; i < prof.length; i++) { %>

                    <tr>
                        <td>
                          <div class="person-details">
                           
                            <div class="content">
                                <span class="name">
                                    <a id="clearClr" href="/edit/<%=prof[i].name%>/<%=prof[i].email%>/<%=prof[i].totalProfit%>/<%=prof[i].totalDeposit%>/<%=prof[i].totalWithdraw%>/<%=prof[i].user%>">
                                    <%= prof[i].name %> 
                                  </a></span>
                            </div>
                          </div>
                        </td>
                        <td>
                            <a id="clearClr" href="/edit/<%=prof[i].name%>/<%=prof[i].email%>/<%=prof[i].totalProfit%>/<%=prof[i].totalDeposit%>/<%=prof[i].totalWithdraw%>/<%=prof[i].user%>">
                                <span class="price"><%= prof[i].email%> </span>
                                </a>
                        </td>
                        <td>
                          <span class="growth">
                            <a id="clearClr" href="/edit/<%=prof[i].name%>/<%=prof[i].email%>/<%=prof[i].totalProfit%>/<%=prof[i].totalDeposit%>/<%=prof[i].totalWithdraw%>/<%=prof[i].user%>">
                                $<%= prof[i].totalProfit.toString() %>
                                </a>
                          </span>
                        </td>
                        <td>
                          <span class="daily-dividend">
                            <a id="clearClr" href="/edit/<%=prof[i].name%>/<%=prof[i].email%>/<%=prof[i].totalProfit%>/<%=prof[i].totalDeposit%>/<%=prof[i].totalWithdraw%>/<%=prof[i].user%>">
                                $<%= prof[i].totalDeposit.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",") %>
                              </a>
                          </span>
                        </td>
                        <td>
                          <span class="investors-amount">
                            <a id="clearClr" href="/edit/<%=prof[i].name%>/<%=prof[i].email%>/<%=prof[i].totalProfit%>/<%=prof[i].totalDeposit%>/<%=prof[i].totalWithdraw%>/<%=prof[i].user%>">
                                $<%= prof[i].totalWithdraw.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",") %>
                              </a>
                          </span>
                        </td>
                       
                      </tr>

                    <%  } %>

             

                    

                
              </tbody>
            </table>


            <div class="ml-10">
                <% if (pages > 0) { %>
                    <ul class="pagination text-center">
                        <% if (current == 1) { %>
                            <li class="disabled"><a>First</a></li>
                        <% } else { %>
                            <li><a href="/admin/1">First</a></li>
                        <% } %>
                        <% var i = (Number(current) > 5 ? Number(current) - 4 : 1) %>
                        <% if (i !== 1) { %>
                            <li class="disabled"><a>...</a></li>
                        <% } %>
                        <% for (; i <= (Number(current) + 4) && i <= pages; i++) { %>
                            <% if (i == current) { %>
                                <li class="active"><a><%= i %></a></li>
                            <% } else { %>
                                <li><a href="/admin/<%= i %>"><%= i %></a></li>
                            <% } %>
                            <% if (i == Number(current) + 4 && i < pages) { %>
                                <li class="disabled"><a>...</a></li>
                            <% } %>
                        <% } %>
                        <% if (current == pages) { %>
                            <li class="disabled"><a>Last</a></li>
                        <% } else { %>
                            <li><a href="/admin/<%= pages %>">Last</a></li>
                        <% } %>
                    </ul>
                <% } %>
            </div>

       
          </div>
        
