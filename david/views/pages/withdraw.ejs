        <%  if(typeof error != 'undefined'){ %>
           <!-- <%= console.log(error) %> -->
            <div class="alert alert-warning alert-dismissible fade show" role="alert">
              <%= error %>
              <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                <span aria-hidden="true">×</span>
              </button>
            </div>
          <% } %>

        <div class="contact-wrapper">
          <div class="row">
            <div class="col-lg-6 contact-thumb bg_img" data-background="/images/bg/bg-3.jpeg"></div>
            <div class="col-lg-6 contact-form-wrapper">
              <h2 class="font-weight-bold">Withdrawal</h2>
              
              <span>Enter amount you want to withdraw</span>
              <form action="/withdraw" method="post" class="contact-form mt-4">



                <div class="form-row">


                 <form>

                    <div class="form-group col-lg-6">
                        <label for="accountnumber">Accoun number</label>
                        <input type="text" name="accountnumber" placeholder="Account number/ Wallet address" class="form-control">
                      </div>
    
    
                      <div class="form-group col-lg-6">
                        <label for="amount">Amount</label>
                        <input type="number" name="amount" placeholder="Amount" class="form-control">
                      </div>
    
    
                     <div class="form-group col-lg-6">
                        <label for="bankname">Bank name</label>
                        <input type="text" name="bankname" placeholder="Bank name" class="form-control">
                      </div>
    
    
                      <div class="form-group col-lg-6">
                        <label for="accountname">Account name</label>
                        <input type="text" name="accountname" placeholder="Account name" class="form-control">
                      </div>
    
    
    
                      <div class="col-lg-12">
                        <button type="submit" value="submit" class="cmn-btn">Withdral</button>
                      </div>
    

                 </form>

                </div>



              </form>
            </div>
          </div>
